<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="main_gridpane" alignment="center" hgap="10" opacity="0.75" stylesheets="@style.css" vgap="10" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Button fx:id="_runSentimentAnalysis" alignment="CENTER" mnemonicParsing="false" onAction="#onRunSentimentAnalysisPressed" stylesheets="@style.css" text="Run Sentiment Analysis" textAlignment="CENTER" GridPane.columnIndex="1" />
      <Button fx:id="_onViewAnalysisResults" alignment="CENTER" mnemonicParsing="false" onAction="#onViewAnalysisResultsPressed" prefHeight="27.0" prefWidth="162.0" stylesheets="@style.css" text="View Analysis Results" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="2" />
   </children>
</GridPane>
